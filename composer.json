{
    "name": "atlas/transit",
    "type": "library",
    "description": "A datasource-domain mapping system.",
    "keywords": [ "domain", "mapper", "mapping", "orm" ],
    "homepage": "http://github.com/atlasphp/Atlas.Transit",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlas Contributors",
            "homepage": "https://github.com/atlasphp/Atlas.Transit/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "atlas/orm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Atlas\\Transit\\": "src/"
        }
    },
    "require-dev": {
        "atlas/testing": "^1.0",
        "pds/skeleton": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Atlas\\Transit\\": "tests/",
            "Atlas\\Orm\\": "vendor/atlas/orm/tests/"
        }
    }
}
